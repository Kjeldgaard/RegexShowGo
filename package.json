{
	"name": "vsc-hyperlink",
	"displayName": "vsc_hyperlink",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vsc-hyperlink.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vsc-hyperlink.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "VSC Hyperlink",
				"properties": {
					"config.match": {
						"type": "string",
						"default": "default string",
						"description": "Regex match pattern"
					},
					"config.array": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"match_pattern": {
									"type": "string",
									"description": "match of inner object"
								},
								"prefix": {
									"type": "string",
									"description": "prefix of inner object"
								}
							}
						},
						"default": ["a", "b"]
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
